{
  "manifest_version": 3,
  "name": "YouTube Videos Sorter",
  "version": "1.0",
  "description": "過去1ヶ月以内に公開されたYouTubeチャンネルの動画とライブを、古い順に一覧表示します。",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
